{
  "name": "sherwood_backend",
  "version": "1.0.0",
  "description": "Nodejs back end to sherwood",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/main/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec sucrase-node ./src/main/server.ts",
    "stage": "cross-env NODE_ENV=stage nodemon --exec sucrase-node ./src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:watch": "npm test -- --watch ",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "build": "tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "mime-types": "^2.1.31",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "password-generator": "^2.3.2",
    "pg": "^8.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/formidable": "^1.2.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/knex": "^0.16.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^16.3.2",
    "@types/nodemailer": "^6.4.4",
    "@types/sqlite3": "^3.1.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "sucrase": "^3.19.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "author": "Lucas Fonseca Barreto",
  "license": "ISC"
}
